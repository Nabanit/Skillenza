				<script>
				$( document ).ready(function() {
					var tditems = $("td[aria-describedby='list_status']");
						for(var x = 0; x < tditems.length; x++){
							var tditem = tditems[x];
							var statuses = tditem['title'].split(',');
							var s = document.createElement('select');

							for(var y = 0; y < statuses.length; y++){
								var o = document.createElement('option');
								o['value'] = statuses[y];
								o.innerText = statuses[y];
								s.appendChild(o);
							}
							tditems[x].innerText = '';
							tditems[x].appendChild(s);

						}
					});
				</script>		</html>
